# Application settings
APP_NAME=ValueCell
APP_VERSION=0.1.0
APP_ENVIRONMENT=development
API_DEBUG=true

# API settings
API_ENABLED=true
API_I18N_ENABLED=true
API_HOST=localhost
API_PORT=8000

# Ensure UTF-8 encoding
# i18n settings, different locales can be set here.
# Database only supports UTF time and English Data.
LANG=en_US.UTF-8
TIMEZONE=

# Model Provider settings
# Currently only OpenRouter is supported.
# You can get your API key from https://openrouter.ai/
OPENROUTER_API_KEY=

# Agent Settings
AGENT_DEBUG_MODE=false

# Model IDs for OpenRouter
PLANNER_MODEL_ID=google/gemini-2.5-pro
SEC_PARSER_MODEL_ID=openai/gpt-4o-mini
SEC_ANALYSIS_MODEL_ID=deepseek/deepseek-chat-v3-0324
AI_HEDGE_FUND_PARSER_MODEL_ID=google/gemini-2.5-flash

# SEC Agent Configuration
# Email address for SEC API requests (required by SEC)
SEC_EMAIL=
SEC_MAX_FILINGS=5
SEC_REQUEST_TIMEOUT=30
